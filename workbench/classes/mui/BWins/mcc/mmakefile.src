%build_module mmake=contrib-zune-classes-bwin-mcc \
  modname=BWin modtype=mcc moduledir=Extras/Zune/MCC_BWin/Classes/Zune \
  files=$(FILES) \
  uselibs="popupmenu"


#MM includes-copy

INCLUDE_FILES := ../include/MUI/BWin_mcc.h
%copy_includes path=mui dir=../include/MUI

%common
